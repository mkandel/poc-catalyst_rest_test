## Format - fieldname:type
id:INTEGER PRIMARY KEY ASC
assetTag:TEXT
comments:TEXT
consoleHostName:TEXT
macAddrConsole:TEXT
consoleServer:TEXT
consoleServerPort:INTEGER
cpuCount:INTEGER
cpuSpeed:INTEGER
datacenter:TEXT
defaultRoute:INTEGER
dnsDomain:TEXT
hardwareType:TEXT
arch:TEXT
hardwareVendor:TEXT
hostname:TEXT
ipAddr:TEXT
ipAddrSecondary:TEXT
lastUpdated:TEXT
macAddr:TEXT
macAddrSecondary:TEXT
macAddrTernary:TEXT
macAddrQuadrary:TEXT
maintenance:TEXT
memorySize:INTEGER
monserverForDatacenter:TEXT
netmask:TEXT
networkSwitch:TEXT
networkSwitchPort:INTEGER
networkSwitchPortSecondary:INTEGER
networkSwitchSecondary:INTEGER
os:TEXT
osPatchLevel:TEXT
osVersion:TEXT
owner:TEXT
ownerTMID:INTEGER
providesServices:TEXT
rackNumber:TEXT
rackPorts:TEXT
rackPosition:TEXT
serialNumber:TEXT
cageNumber:TEXT
status:TEXT
